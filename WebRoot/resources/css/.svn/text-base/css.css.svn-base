.page-wrapper{padding-top:8%;width:880px;margin-left:auto;margin-right:auto}
.ui-grid-row{margin-bottom:10px;*zoom:1}
.ui-grid-row:after{clear:both;content:" ";display:block;font-size:0;height:0;visibility:hidden}
.account-head{padding-top:17px}
.account-head a{text-decoration:none}
.account-head img{width:270px;height:62px}
.account-head-font{position:relative;bottom:20px;left:10px;font-size:25px;padding-left:5px;height:50px;line-height:50px;display:inline-block;border-left:1px solid #ccc;color:#666}

.registerNew
{
    border: 1px solid #E8E8E8;
    border-top: 5px solid #FF9900;
    padding: 80px 0px 60px;
    background-color: white;
}

    .registerNew .ui-button
    {
        font-size: 12px;
    }

    .registerNew .reg-form-item
    {
        padding-bottom: 10px;
    }

.reg-form-item .reg-label
{
    float: left;
    width: 150px;
    text-align: right;
    color: #666;
    margin-top: 4px;
    margin-right: 20px;
}

.reg-form-item .reg-textarea
{
    float: left;
    height: 60px;
    width: 300px;
}

.reg-form-item .reg-input
{
    width: 300px;
    height: 25px;
    line-height: 25px;
    border: 1px solid #ccc;
    float: left;
    vertical-align: middle;
    padding-left: 3px;
}

.reg-form-item .reg-input.currency
{
    width: 190px;
}

.reg-form-item .reg-required-span
{
    margin-top: 5px;
    width: 20px;
    float: left;
    text-align: center;
}


.reg-form-item .reg-explain
{
    color: #999;
    float: left;
    margin-top: 4px;
}

.reg-form-item .reg-explain-show
{
    color: #ccc;
    float: left;
    margin-top: 4px;
}

.reg-form-item.item-agree
{
    padding-left: 400px;
}

    .reg-form-item.item-agree div
    {
        float: left;
    }

    .reg-form-item.item-agree .reg-explain
    {
        width: 200px;
        margin-top: 0px;
    }

    .reg-form-item.item-agree input[type=checkbox]
    {
        vertical-align: middle;
        margin-right: 5px;
        margin-top: -2px;
    }

.reg-form-item.item-center
{
    text-align: center;
}

.reg-form-item .reg-btn
{
    width: 160px;
    height: 45px;
}

.ui-button-lblue {
    line-height: 40px;
    height: 40px;
}
.ui-button {
	display:inline-block;
	*display:inline;
	*zoom:1;
	*overflow:visible;
	text-align:center;
	text-decoration:none;
	vertical-align:middle;
	cursor:pointer;
	font-size:14px;
	font-weight:700;
	padding:0 20px;
	background-image:none;
    border:0;
}
.ui-button .ui-button-text {
	background:0 0;
	padding:0;
	height:auto;
	line-height:auto;
}
.ui-button:hover {
	text-decoration:none;
	background-image:none
}

.ui-button-lblue {
    color: #fff;
    background: #3683ec;
}
.form-required {
    color: red;
    margin-left: 3px;
}
