/* 起始样式 */
body {font-family:"微软雅黑";  margin:0 auto; padding:0px; font-size:12px; height:100%; line-height:22px; width:100%;}
div{behavior: url(css/iepngfix.htc)}
ul,ul li,ol li,tr td,dl dd,dd{ list-style-type:none; margin:0; padding:0; }
h1,h2,h3,h4,h5,h6{ margin:0; padding:0;}
p{ margin:0; padding:0}
.clear{ clear:both; height:0px; line-height:0px; font-size:0px;}
a{ text-decoration:none; cursor:pointer;}
/*a:hover{ text-decoration:underline;}*/
i{ font-style:normal;}
img{ vertical-align:middle;}
.imgs{ vertical-align:top;}
.lf{ float:left;}
.rg{ float:right;}  
img{border:none;}
html{ overflow-x:hidden;} 
div:hover,li:hover,tr:hover,a:hover{ -webkit-transition: none; -moz-transition: none; transition: color 0.3s linear, background-color 0.3s linear, background-image 0.3s linear; -moz-transition: color 0.3s linear, background-color 0.3s linear, background-image 0.3s linear; -webkit-transition: color 0.3s linear, background-color 0.3s linear, background-image 0.3s linear; -o-transition: color 0.3s linear, background-color 0.3s linear, background-image 0.3s linear;}
/* 起始样式 End */

/* 公用样式 */
.wripe{ background:url(../images/bg.jpg) no-repeat; -moz-background-size:100% 100%; background-size:100% 100%; width:100%; height:100%; position:fixed;}
.head{ height:57px; z-index:1000; width:100%; border-bottom:1px solid #cccccc;}
.box{ max-width:1200px; margin:0 auto; min-width:500px;}
.logo{ width:292px; height:31px; margin-top:14px; margin-bottom:10px; float:left;}
.logo a{ display:block;}
.headRg{ width:250px; float:right; overflow:hidden; margin-top:26px;}
.headRg a{ display:block; float:left; width:60px; padding-left:28px; font-size:14px; color:#4d4d4d;}
.headRg a:hover{ color:#aaa;}
.headRg .a1{ background:url(../images/icon.png) no-repeat left center; margin-right:6px;}
.headRg .a2{ background:url(../images/icon2.png) no-repeat left center; padding-left:24px;}
.headRg .a3{ background:url(../images/find.png) no-repeat left center; margin-right:24px;}

.footer{ width:100%; position:fixed; bottom:0; border-top:1px solid #ccc;font-family:Microsoft yahei;font-size:14px;}
.fotcent{ width:1200px; margin:0 auto; position:relative; left:50%; margin-left:-600px;}
.fotcent p{ width:100%;color:#ccc; text-align:center; line-height:40px;}
.fotcent a{ color:#ccc;}
/* 公用样式 End */

/* Index */
.centBox{ width:580px; height:266px; background:#fff; position:absolute; left:50%; margin-left:-290px; top:50%; margin-top:-133px; box-shadow:0 0 8px #444; overflow:hidden;}
.centBox .leftImg{ float:left;}
.load{ width:298px; float:left; margin-left:46px;}
.load .loadtt{ padding:24px 0; font-size:16px; color:#333333;}
.load .loadtt i{ font-size:12px; color:#008cd6; text-transform:uppercase;}
.load input{ outline:none; border:0 none; margin-left:0;}
.load .user{ margin-bottom:20px;}
.load .user input{  height:43px; background:url(../images/img2.jpg) no-repeat left center; padding-left:36px; line-height:43px; font-family:"微软雅黑";color:#8fa1bb;  }
.load .password{ margin-bottom:20px;}
.load .password input{ height:43px; background:url(../images/img3.jpg) no-repeat left center; padding-left:36px; line-height:43px; color:#8fa1bb; }
.load .loadbt{ overflow:hidden;}
.load .loadbt .btn{ float:left; width:95px; margin-right:10px;}
.load .loadbt .memb{ float:left; width:95px; padding-top:6px; color:#666; font-size:12px;}
.load .loadbt .key{ float:left; color:#97a5b2; display:block; width:60px; padding-top:6px;}
.load .loadbt .key:hover{ text-decoration:underline; color:#4196c9;}
/* Index End */ 
/* 后台SRM */
.s_head{ width:100%; height:50px; color:#fff; background:#008cd6;}
.s_head a{ color:#fff;}
.s_head .s_logo{ width:135px; float:left;}
.s_head .s_s{  text-align:center; padding-left:10px;line-height:50px; float:left; font-size:14px;}
.s_head .s_topR{ width:380px; float:right; margin-right:50px; display:table; padding-top:20px;}
.s_head .s_topR li{ display:table-cell; background:url(../images/line3.jpg) no-repeat right center;}
.s_head .s_topR li a{ display:block; text-align:center; margin:0 8px;}
.s_head .s_topR li .s_a1{ padding-right:10px; background:url(../images/icon9.png) no-repeat right center;}
.s_head .s_topR li a:hover{ color:#8cc3ea;}

.bigBox{ width: 100%}
.bigBox .s_aside{ width:100%; float:left; background:#282b32; color:#fff; position:relative;}
.bigBox .s_aside .s_ft{ width:100%; height:40px; background:#30363f; position:absolute; bottom:0px; left:0;}
.bigBox .s_right{ width:81.39%; float:left; position:relative; background:#eaebf0;}
.bigBox .s_right .s_content{ width:96.6%; padding:2.47% 1.7%; text-align:center;}
.accordion { width:100%; background:#282b32;}
.accordion .link { cursor: pointer; display: block; overflow:hidden; padding: 0 4%; color: #fff; font-size: 14px; line-height:40px; height:40px; border-bottom: 1px solid #353841; position: relative; -webkit-transition: all 0.4s ease; -o-transition: all 0.4s ease; transition: all 0.4s ease; }
.accordion li:last-child .link { border-bottom: 0;}
.accordion li{ border-bottom: 1px solid #1a1c20;}
.accordion li p{ display:inline; float:left;}
.accordion li i { display:block; float:left; width:20px; height:20px; margin:10px 4% 10px 0; font-size: 18px; color: #595959; -webkit-transition: all 0.4s ease; -o-transition: all 0.4s ease; transition: all 0.4s ease; }
.accordion li i.fa-chevron-down1 { background:url(../images/icon8.png) no-repeat center; float:right;}
.accordion li i.fa-paint-supplier{  background:url(../images/icon4.png) no-repeat center;}
.accordion li i.fa-sys-config{ background:url(../images/icon5.png) no-repeat center;}
.accordion li i.fa-cooperation{ background:url(../images/icon6.png) no-repeat center;}
.accordion li i.fa-globe{ background:url(../images/icon7.png) no-repeat center;}
.accordion li.open .link { color: #fff;}
.accordion li.open i {color: #b63b4d;}
.accordion li.open i.fa-chevron-down1 { -webkit-transform: rotate(90deg); -ms-transform: rotate(90deg); -o-transform: rotate(90deg); transform: rotate(90deg);
}
/*** Submenu ***/
 .submenu { display: none; background: #1a1c20; font-size: 14px; }
 .submenu li{ border-bottom:none;}
 .submenu li:hover {background:url(../images/line2.jpg) repeat-x;}
 .submenu a { display: block; text-decoration: none; color: #fff; padding: 12px; padding-left:16%; font-size:12px; position:relative; -webkit-transition: all 0.25s ease; -o-transition: all 0.25s ease; transition: all 0.25s ease; }
 .submenu a:hover { background:url(../images/line1.jpg) no-repeat left center;}
.submenu li a i{ display:block; width:6px; height:6px; background:url(../images/icon10.png) no-repeat center; position:absolute; left:6%; top:24%;}

.s_foot{ height:40px; width:100%; line-height:40px; background:#0771aa; text-align:center; position:absolute; left:0; bottom:0;}
.s_foot p{ color:#ffffff;}
/* 后台SRM结束 */

