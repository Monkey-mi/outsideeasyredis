<script id="evaluationtmpl" type="text/x-dot-template">
{{ for(var prop in it) { }}
	<p class="title">{{= it[prop].menu_name }}</p>
	<ul>
	{{ for(var child in it[prop].children){ }}
		{{? it[prop].children[child].id==61}}
			<li><a onclick="go_skip('{{= it[prop].children[child].menu_path}}')" ><img src="/newresources/images/page/new_sale.png" class="f_l" /><span style="margin-left:-40px;">{{= it[prop].children[child].menu_name}}</span></a></li>
		{{??}}
			<li><a onclick="go_skip('{{= it[prop].children[child].menu_path}}')">{{= it[prop].children[child].menu_name}}</a></li>
		{{?}}
	{{ } }}
	</ul>
{{ } }}
</script>
<script type="text/javascript">
$(function(){
var data=getMenuData(15);
var evalText=doT.template($("#evaluationtmpl").text());
$("#evaluation").html(evalText(data));
});

function go_skip(url){
	addParamsToWindowName(null);
	window.location.href=getwebroot()+url.slice(1)+'.htm';
}
</script>
</head>
