/*发货管理样式*/
.deliveryManageList{ overflow:hidden; position:relative; width:auto; min-height:180px;}
.deliveryManageList .tab{ overflow:hidden; border-bottom:2px solid #e7e7e7; }
.deliveryManageList .tab li{ background-color:#f6f6f6; cursor:pointer; float:left; height:34px; width:110px; text-align:center; line-height:34px;color:#333;}
.deliveryManageList .tab li span.split{ color:#e7e7e7; float:right; height:100%; width:auto; font-size:16px; font-weight:bold;}
.deliveryManageList .tab li:hover{ font-weight:bold;background-image:none;}
.deliveryManageList .tab li.curr{font-weight:bold;background-image:none;}
.deliveryManageList .tabcon{ display:none; width:auto; height:auto; overflow:hidden; min-height:100px; padding-bottom:30px;}

.startDelivery{width:100px;height: 30px;line-height: 30px;border-radius:3px;background: #58749C;color: #FFFFFF;right:0px;top:0px;cursor: pointer;text-align:center;}
.searchBtn{width: 55px;height: 30px;background: #58749C;color: #FFFFFF;border-radius: 3px;line-height: 30px;text-align: center;}
.searchCriteria{width: 100%;height: auto;margin-top:15px;}
.searchCriteria label {display:inline-block;width:70px ;height: 30px;line-height: 30px;color:#333;}
.searchCriteria .inputWrap1{width: 180px;height: 28px;border: 1px solid #E8E8E8;line-height:28px;}
.searchCriteria .inputWrap2{width: 80px;height: 28px;border: 1px solid #E8E8E8;line-height:28px;}
.searchCriteria .selectShowWrap{width:170px;height: 28px;line-height: 28px;border: 1px solid #E8E8E8;color:#999;padding-left:8px;}
.searchCriteria .selectWrap{border:1px solid #e8e8e8;heihgt:auto;left:0px;top:30px;padding-left:10px;display:none;text-align:left;color:#999;background:#fff;z-index:15;}

.deliveryTableList{width:100%;height:auto;table-layout:fixed;}
.deliveryTableList tr{min-height:60px;}
.deliveryTableList th{color:#777;text-align:center;background:#f6f6f6;height:30px;line-height:30px;font-weight:normal;margin-bottom:10px;}
.deliveryTableList th.wrap1{width:80px;}
.deliveryTableList th.wrap2{width:165px;}
.deliveryTableList th.wrap3{width:65px;}
.deliveryTableList th.wrap4{width:60px;}
.deliveryTableList th.wrap5{width:50px;}
.deliveryTableList td{padding:10px 5px 10px 5px;line-height:20px;height:40px;color:#333;border-bottom:1px solid #e8e8e8;}
.deliveryTableList td.center{text-align:center;}
.deliveryTableList .operate{color:#2170e8;}
.deliveryTableList .operate:hover{text-decoration: underline;}
.deliveryTableList .orderNumber{color:#333;text-decoration: underline;}
.deliveryTableList .orderNumber:hover{color:#2170e8}
.deliveryTableList .editWayBill{color:#333;}
.deliveryTableList .editWayBill:hover{color:#2170e8;text-decoration: underline;}
.deliveryTableList .waiting{color:#333;font-weight:bold;}
.deliveryTableList .finished{color:#333;}
.deliveryTableList .canceled{color:#999;}

