@CHARSET "UTF-8";
.purchase_leftMenu{ width:auto; height:auto;background-color:#fff; overflow:hidden;}
.purchase_left_block_title{ width:100%; height:43px; line-height:40px; color:#fff; font-size:16px; background:#8ec165 url(../images/page/buy_t.png) repeat-x;}
.purchase_left_block_title .bg_img{ float:right; margin-right:10px;}

.purchase_leftMenu .title{ height:40px; line-height:40px; border-bottom:1px solid #8ec165; padding-left:10px; font-size:14px;}

.purchase_leftMenu ul{ width:100%; height:auto; }
.purchase_leftMenu ul li{ width:100%; height:40px; text-align:center; line-height:40px;}
.purchase_leftMenu ul li a{ float:left; width:99%; height:40px; line-height:40px; text-align:center; color:#777;}
.purchase_leftMenu ul li a:hover{ background-color:#ececec; text-decoration:none;}
.purchase_leftMenu ul li .curr{background-color:#ececec;}