.verifyTableList td{padding:10px 0px 10px 10px;text-align: left;color:#333;vertical-align:top;border-top:1px solid #e8e8e8;font-size:12px;line-height:18px;}
.verifyTableList td.center{text-align:center;}
.verifyTableList td a{color:#0074c2;}
.verifyTableList td.type{font-weight:bold;color:#555;font-family:'microsoft yahei';}
.verifyTableList .updateTime{color:#999;font-family:'microsoft yahei';}

/*=========审核页面样式==========*/
.c999{color:#999;}
.accessVerifyInfo{padding:20px 10px 120px 10px;}
.accessVerifyInfo .verifyResult{width:784px;height:auto;padding:20px 15px;border:1px solid #efefef;border-radius:3px;box-shadow:0px 0px 7px #ccc;background:#fbfaf9;}
.accessVerifyInfo .verifyResult .title,.verifyRecords .title{font-weight:bold;color:#555;}
.accessVerifyInfo .verifyResult label,.verifyRecords label{display:inline-block;width:60px;height:auto;color:#777;vertical-align:top;}
.accessVerifyInfo .verifyResult .editWrap{display:inline-block;vertical-align:top;}
.accessVerifyInfo .verifyResult .choosed{width:682px;height:auto;margin-left:10px;display:inline-block;}
.accessVerifyInfo .verifyResult .verifyAdvice{width:720px;height:90px;resize:none;border:1px solid #d0d0d0;outline:none;}
.accessVerifyInfo .verifyResult .submit{width:80px;height:25px;border-radius:3px;background:#58749c;color:#fff;margin-left:60px;margin-top:10px;}
.accessVerifyInfo .floatSelectWrap{top:5px;left:-3px;z-index:10;}
.accessVerifyInfo .floatSelectWrap .triangle{margin-left:8px;margin-bottom:-6px;}

.verifyRecords{padding:20px 12px;border:1px solid #e8e8e8;border-radius:3px;box-shadow:0px 0px 7px #ddd;background:#fbfaf9;margin-top:10px;width:790px;}
.verifyRecords .splitLine{color:#999;margin:0px 20px;}
.verifyRecords .record{padding:20px 0px;}
.verifyRecords .record_hr{height:1px;border:none;border-top:1px solid #e8e8e8;}
.verifyRecords .verifyAdvice{width:724px;height:auto;display:inline-block;}

.verifySupplierInfo{border:1px solid #e8e8e8;margin-top:20px;}
.verifySupplierInfo .formTitle{padding:20px 15px 15px 15px;}
.verifySupplierInfo .formTitle .name{font-size:13px;font-weight:bold;color:#333;text-align: center;}
.verifySupplierInfo .formTitle .company ,.verifySupplierInfo .formTitle .submitTime{color:#777;display:inline-block;width:60px;}
#check_cyc_batch .cyc_data_wrap .cyc_radio .cyc_unit{display:inline-block;width:100px;}


