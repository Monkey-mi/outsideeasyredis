/*========遮罩层=========*/
.mask{ position:fixed; top:0px; left:0px; display:none; width:100%; height:100%; background-color:#000;opacity:0.5;filter:alpha(opacity=50);z-index:1;}
/*=========表单提示、验证信息样式===========*/
.info_explain_wrap{ float:left; display:none; margin-left:10px; width:auto; height:32px; line-height:32px;}
.info_explain_wrap .info_explain{ position:relative; width:100%; height:100%;}
.info_explain_wrap .info_explain .info_explain_icon{ position:absolute; left:0px; top:50%; margin-top:-6px; width:11px; height:14px; z-index:10;}
.info_explain .info_explain_tip{position:absolute;left:11px; top:0px;  margin-left:-1px; padding:4px 4px 4px 10px; font-size:12px; color:#666; height:auto; width:140px; border:1px solid #d0d0d0; line-height:22px;border-radius:4px;-webkit-border-radius: 4px; -moz-border-radius: 4px; background-color:#fff;}

/*=======窗口右上角X关闭样式=========*/
 .close_btn{ position:absolute; width:20px; height:30px; line-height:30px; top:0px; right:0px; font-size:16px; color:#fff; font-weight:normal;opacity:0.8;filter:alpha(opacity=80); text-decoration:none;}
 .close_btn:hover{opacity:1;filter:alpha(opacity=100); text-decoration:none; font-weight:bold;}

 
 /*=======公用相对定位层=======*/
.comm_inner_line_wrap{ position:relative; width:auto; min-height:32px; height:auto; margin-bottom:10px;text-align:left;}
.comm_inner_line_wrap .label_wrap{ float:left; width:80px; height:32px; line-height:32px; text-align:left; margin-right:20px; margin-left:20px;}
.comm_inner_line_wrap .noEmpty{ float:left; color:red; width:20px; line-height:32px; height:32px;}

.comm_inner_line_wrap  .input_wrap{ float:left;  width:300px;  height:30px; line-height:30px; border:1px solid #d0d0d0;}
.comm_inner_line_wrap  .input_wrap:hover{ border:1px solid #f1af27;}
.comm_inner_line_wrap  .select_wrap{ float:left; width:310px; height:30px; line-height:30px; border:1px solid #d0d0d0;}
.comm_inner_line_wrap .info_explain{ position:absolute; top:0px; left:444px; color:#666; height:32px; width:auto; line-height:32px; display:none;}
