@charset "utf-8";
/* CSS Document */
/*===webkit内核浏览器禁止自适应字体大小；防止文字换行时被截断===*/
html{ -webkit-text-size-adjust: none;word-wrap:break-word;}
html,body{width: 100%;height: 100%;}
/*==表单元素在手机中点击时防止出现边框==*/
* {-webkit-tap-highlight-color: rgba(0, 0, 0, 0);}
* {margin: 0;padding: 0;}
body {color:#333; font-size:12px; line-height:150%; font-family:"Microsoft YaHei", tahoma, arial, Hiragino Sans GB, \5b8b\4f53; min-width:1024px;}
a {text-decoration: none; color:#333; cursor:pointer;}
a:hover{ text-decoration:underline; }

ul, ol, li { list-style: none;}
ol, ul{ list-style: none outside none;}
em, i {font-style: normal;}
h1,h2,h3,input,button,textarea,select{font-size:12px;}
img { border: none;}
input{padding-left:8px; background:#fff; border:1px solid #fff;}
input:hover{border:1px solid #fecb00;}
input[type=checkbox] {
	padding-left:0px;
}

table{border-collapse:collapse;border-spacing:0;empty-cells:show;font-size:inherit}
td{ line-height:200%; font-size:100%; }
th{ line-height:200%; font-size:100%;}
button{border:none;box-shadow:0;-moz-box-shadow:0;  -webkit-box-shadow:0;cursor:pointer;}
button::-moz-focus-inner {
    border:0;
    padding:0;
}
li,img,label,input{vertical-align:middle}
select {
  /*=====Chrome和Firefox里面的边框是不一样的，所以复写了一下===*/
  border:1px solid #000;

  /*===很关键：将默认的select选择框样式清除===*/
  appearance:none;
  -moz-appearance:none;
  -webkit-appearance:none;
  /*===在选择框的最右侧中间显示小箭头图片===*/
  background: url(../images/icon/arrow.png) no-repeat scroll right center transparent;
 /*===为下拉小箭头留出一点位置，避免被文字覆盖===*/
  
  padding-left:8px;
}
/*==清除ie的默认选择框样式清除，隐藏下拉箭头==*/
select::-ms-expand { display: none; }

.clear{ clear:both;}
.clearfix:after {display: block; content: ''; clear: both; visibility: hidden; }
