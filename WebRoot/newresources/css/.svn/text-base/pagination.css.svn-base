.pagination {
      /*       font-size: 80%; */
            float:right;
            font-size:11px;
            line-height:11px;
        }
        
.pagination a {
    text-decoration: none;
	border: solid 1px #AAE;
	color: #15B;
}

.pagination a, .pagination span {
    display: block;
    float: left;
    padding: 0.3em 0.5em;
    margin-right: 0px;
	margin-bottom: 4px;
	min-width:1em;
	text-align:center;
}

.pagination .current {
    background: #26B;
    color: #fff;
	border: solid 1px #AAE;
}

.pagination .current.prev, .pagination .current.next{
	color:#777;
	border-color:#d0d0d0;
	background:#fff;
}
.pagination .current.first{
/* 	background-color:#58749c;
	color:#fff; */
	border:1px solid #58749c;
	background:#fafafa;
	color:#58749c;
	width:30px;
	font-weight:bold;
}
.pagination .first{
	/* background-color:#58749c;
	color:#fff; */
	border:1px solid #d0d0d0;
	background:#fafafa;
	color:#777;
}

.pagination .display_span{ color:#777;margin-right:4px;line-height:11px;font-size:11px;}
.pagination .display_span .records{color:#58749c;font-size:11px;font-weight:bold;margin-left:4px;margin-right:4px;vertical-align:top;}
.pagination .go_to_text{float:left; border:1px solid #ddd; width:13px;color:#777;margin-right:4px;}
.pagination input{ padding-left:5px;}
.pagination .go_to_button{float:left; background-color:#fafafa;color:#777;font-size:11px;width:17px;height:17px;border:1px solid #d0d0d0;}
.pagination .go_to_button:hover{border:1px solid #58749c;background:#fafafa;color:#58749c;font-weight:bold;}
.pagination .go_to_button:active{border:1px solid #58749c;background:#fafafa;color:#58749c;font-weight:bold;}
