div.quotes {
   margin: 3px; text-align: center; font-family:Microsoft YaHei; font-size:11px;
}
div.quotes a {
    border: #d0d0d0 1px solid; padding:3px 5px;color: #777; margin-right: 4px;  text-decoration: none;background:#fff;font-size:11px;
}
div.quotes a:hover {
    border:#58749c 1px solid; padding:3px 5px;color: #58749c; margin-right: 4px;  text-decoration: none;background:#fafafa;font-size:11px;font-weight:bold;
}
div.quotes a:active {
    border:#d0d0d0 1px solid; padding:3px 5px;color: #fff; margin-right: 4px;  text-decoration: none;background:#58749c;font-size:11px;
}
div.quotes span.current{
	border:1px solid #58749c;background:#fff;color:#58749c;padding:3px 5px;margin-right:4px;font-size:11px;font-weight:bold;
}
div.quotes span.disabled {
    border-right: #f3f3f3 1px solid; padding-right: 5px; border-top: #f3f3f3 1px solid; padding-left: 5px; padding-bottom: 2px; border-left: #f3f3f3 1px solid; color: #ccc; margin-right: 2px; padding-top: 2px; border-bottom: #f3f3f3 1px solid
}